<?xml version="1.0" encoding="utf-8"?>
<Configuration>
    <!-- URL to Server/Site to sign into-->
    <SiteUrl value="https://10ay.online.tableau.com/#/site/xxxxxxxx"/>

    <!-- Authentication for the Tableau site-->
    <!--EXAMPLE USING EMAIL AND PASSWORD: <TableauSiteLogin signInMode="NameAndPassword" clientId="xxxxx@xxxx.com" secret="xxxxxxxx"/>-->
    <TableauSiteLogin signInMode="PersonalAccessToken" clientId="xxxxxxxxxxxxxxx" secret="xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx" />


    <!--Authentication for the Azure AD instance we need to sign into to query group/members information -->    
    <AzureAdLogin tenantId="xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx" clientId="xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx" secret="xxxxxxxxxxxxxx" />
</Configuration>
